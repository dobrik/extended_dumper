{
  "name": "dobrik/extended-dumper",
  "type": "library",
  "description": "Symfony var dumper with extended data.",
  "keywords": ["dump", "variable", "class", "property"],
  "homepage": "https://github.com/dobrik",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "symfony/var-dumper": ">=4.0"
  },
  "autoload": {
    "files": [ "src/functions.php" ],
    "psr-4": { "Dobrik\\ExtendedDumper\\": "src/" }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "master-dev"
    }
  }
}
